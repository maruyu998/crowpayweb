{
  "name": "crowpayweb",
  "version": "1.0.0",
  "description": "",
  "main": "server/index.ts",
  "config": {
    "dev_react_port": "3001",
    "dev_server_port": "3002"
  },
  "scripts": {
    "graphql:codegen": "VERBOSE=1 DEBUG=1 graphql-codegen --config codegen.yml",
    "rdev": "PORT=${npm_package_config_dev_react_port} PROXYPORT=${npm_package_config_dev_server_port} webpack serve",
    "rdep": "npx tailwindcss -i ./client/src/styles.css -o ./client/public/output.css && webpack build --progress",
    "tsc": "tsc",
    "sdev": "nodemon server/index.ts",
    "sdep": "tsc && NODE_ENV=production npx ts-node --transpile-only server/index.ts",
    "dev": "run-p rdev sdev",
    "dep": "npm run rdep && npm run sdep",
    "forever": "NODE_ENV=production forever start --minUptime 60000 --spinSleepTime 20000 --uid ${npm_package_name} -a -c 'ts-node --transpile-only' server/index.ts"
  },
